{
    "name": "lopezsoft/pdf-excel-generator",
    "description": "Generate PDFs and Excel files from HTML/Blade templates or data arrays for Laravel",
    "type": "library",
    "license": "MIT",
    "version": "1.2.9",
    "keywords": [
        "pdf",
        "excel",
        "laravel",
        "blade",
        "generator",
        "export",
        "puppeteer",
        "browsershot",
        "phpspreadsheet"
    ],
    "support": {
        "issues": "https://github.com/lopezsoft/pdf-excel-generator/issues",
        "source": "https://github.com/lopezsoft/pdf-excel-generator"
    },
    "authors": [
        {
            "name": "Lewis Lopez",
            "email": "lopezsoft.com@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "spatie/browsershot": "^3.57|^4.0",
        "phpoffice/phpspreadsheet": "^1.29|^2.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "illuminate/contracts": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Lopezsoft\\PdfExcelGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lopezsoft\\PdfExcelGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lopezsoft\\PdfExcelGenerator\\PdfExcelGeneratorServiceProvider"
            ],
            "aliases": {
                "PdfExcelGenerator": "Lopezsoft\\PdfExcelGenerator\\Facades\\PdfExcelGenerator"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}